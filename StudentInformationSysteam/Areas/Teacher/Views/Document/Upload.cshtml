
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Teacher/Views/Shared/_TeacherLayout.cshtml";
}
@model UploadVM
<section class="my-3">
    <div class="container">
        <div class="row">
            <div class="col-lg-8">
<form method="post" enctype="multipart/form-data">
  <div class="mb-3">
    <label asp-for="File" class="form-label"></label>
    <input asp-for="File"    class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
   <span asp-validation-for="File"></span>
  </div>
  <div class="mb-3">
    <label asp-for="Title" class="form-label"></label>
    <input asp-for="Title" class="form-control" id="exampleInputPassword1">
     <span asp-validation-for="Title"></span>
  </div>
  
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
</div>
</div>
</div>
</section>